import string
a=input("Enter Password: ")
score=0
if len(a)>=8 :
    score+=1
if len(a)>=12:
    score+=1
if any(c.islower() for c in a ):
    score+=1
if any(c.isupper() for c in a ):
    score+=1
if any(c.isdigit() for c in a):
    score+=1
if any(c in string.punctuation for c in a):
    score+=1
if a.isdigit() or a.islower() or a.isupper() or all(c in string.punctuation for c in a):
    score-=1
if len(set(a))==1:
    score=0
if score <= 2:
    strength = "Weak "
elif score <= 4:
    strength = "Medium"
else:
    strength = "Strong"

print(f"Password strength: {strength}") 


 
