import random
import string 
a=int(input("Enter length of password: "))
if a<7 or a>16:
    print("Out of range")
else:
 char_password=(random.sample(string.ascii_letters,  k=a-3) + random.choices(string.digits, k=2) +
               random.choices(string.punctuation, k=1) )
 random.shuffle(char_password)
 password=''.join(char_password)
 print(password)
