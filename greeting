import time

current_hour = int(time.strftime('%H'))

print(f"The current time is: {time.strftime('%H:%M:%S')}")


if 5 <= current_hour < 12:
    print("Good MORNING")
elif 12 <= current_hour < 17:
    print("Good AFTERNOON")
elif 17 <= current_hour < 20: 
    print("Good EVENING")
else:
    print("Good NIGHT")
